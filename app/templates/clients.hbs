{{#transition-group transition-class="SlideIn"}}
	<h1>{{model.title}}</h1>
	<h2>{{model.intro}}</h2>
	<hr>
	<section class="align-left-top primary-content">
		<div class="flex-wrap">
		{{#each model.list as |item|}}
		<div class="card-wrap col-1-4">
			<div class="card"><h6>{{item}}</h6></div>
			</div>
		{{/each}}
		</div>
	</section>
{{/transition-group}}

