{{#transition-group transition-class="SlideIn"}}
	<h1>{{model.title}}</h1>
	<h2>{{model.intro}}</h2>
	<hr> 
	<section class="align-left-top primary-content">
		<ul>
		{{#each model.qualities.list1 as |item|}}
			<li>{{item}}</li>
		{{/each}}
		</ul>
		<ul>
		{{#each model.qualities.list2 as |item|}}
			<li>{{item}}</li>
		{{/each}} 
		</ul>
	</section>
{{/transition-group}}